[user]
    name = Brett Sun
    email = qisheng.brett.sun@gmail.com
[core]
    excludesfile = .gitignore_global
    autocrlf = input
[credential]
    helper = osxkeychain
[push]
    default = simple
[commit]
    gpgsign = true
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
[alias]
    cleanup = "!git branch --merged | grep  -v '\\*\\|master\\|develop' | xargs -n 1 git branch -d"
    unstage = "reset HEAD --"
